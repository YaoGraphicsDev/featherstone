      <Item Name="Rows">6</Item>
      <Item Name="Columns">6</Item>
      <ArrayItems>
        <Size>36</Size>
        <ValuePointer>(float*)m_storage.m_data.array</ValuePointer>
      </ArrayItems>
    </Expand>
  </Type>

  <!-- MCoordinates, FCoordinate - Eigen::Matrix<float, -1, 1, 0, 6, 1> -->
